@model Acme.ProductSelling.Specifications.CaseSpecificationDto
@using Microsoft.Extensions.Localization
@using Acme.ProductSelling.Localization
@inject IStringLocalizer<ProductSellingResource> L

<h5>@L["Case Details"]</h5>
<dl class="row mt-3">
    <dt class="col-sm-4">@L["SupportedMbFormFactor"]</dt>
    <dd class="col-sm-8">@Model.SupportedMbFormFactor</dd>
    <dt class="col-sm-4">@L["Color"]</dt>
    <dd class="col-sm-8">@Model.Color</dd>
    <dt class="col-sm-4">@L["MaxCpuCoolerHeight"]</dt>
    <dd class="col-sm-8">@Model.MaxCpuCoolerHeight</dd>
    <dt class="col-sm-4">@L["MaxGpuLength"]</dt>
    <dd class="col-sm-8">@Model.MaxGpuLength</dd>

</dl>